readquery <- function()
{ 
prompt <- "Digite a consulta: "
  n <- readline(prompt)
  n <- (unlist(strsplit(n, "\\s+")))
  return(n)
}

consulta<- function() {
rq<-tolower(readquery())

for (i in 1:length(rq)){
	ccc <- (paste(rq[i],(as.numeric(as.character(item1[item1$Termos==rq[i],c('idf')])))))
	print(ccc)
}
}
